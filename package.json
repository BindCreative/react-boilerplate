{
  "name": "bind-react-ts-boilerplate-",
  "version": "0.1.0",
  "description": "React TS boilerplate",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn build:clean",
    "build:dev": "cross-env NODE_ENV=development webpack --config internal/webpack/webpack.dev.babel.js --color -p --progress --hide-modules --display-optimization-bailout --watch",
    "build:prod": "cross-env NODE_ENV=production webpack --config internal/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' '__test__/**/*.{ts,tsx,js,jsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}' && yarn lint:eslint --fix",
    "format:check": "prettier --check './**/*.{ts,tsx,js,jsx}'",
    "test": "jest test --updateSnapshot"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "jest": "^25.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.13.0",
    "styled-components": "^5.0.1",
    "webpack": "^4.42.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "circular-dependency-plugin": "^5.2.0",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.1",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "fork-ts-checker-webpack-plugin": "^4.0.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest-config": "^25.1.0",
    "lint-staged": "^10.0.8",
    "offline-plugin": "^5.0.7",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-app-polyfill": "^1.0.6",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "typescript-plugin-styled-components": "^1.4.4",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-pwa-manifest": "^4.2.0"
  }
}
